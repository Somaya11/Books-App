<%- include('../partials/header') %>

<h1> TICKETS</h1>

<form action="/flights/<%= flight._id %>/tickets" method="POST">
  <label>Seat (A1 - F9):
    <input name="seat" required pattern="[A-F][1-9]\d?">
</label><br>
<label>Price:
    <input type="number" name="price">
</label><br>
<button type="submit">Submit Ticket</button>
</form>
  

